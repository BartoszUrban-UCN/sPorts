@model WebApplication.Models.Spot

@{
    ViewData["Title"] = "Spot";
}

@section Styles{
    <link rel="stylesheet" href="~/css/marina.css"/>
}

@if (Model != null)
{
<div class="container-fluid">
    <div class="card-columns">
            <div class="card test-white">
                <img class="card-img" src="~/img/1.webp">
                <div class="card-body">
                    <h5 class="card-title">@Model.Price</h5>
                    <p class="card-text">@Model.SpotId</p>
                </div>
            </div>
    </div>
</div>
}
else
{
    <h1>No spot found</h1>
}